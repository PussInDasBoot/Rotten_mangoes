<h1>Admin page for all users</h1>

<%= link_to "Add a new user", new_admin_user_path %>

<%= paginate @users %>

<% @users.each do |user| %>
<p><%= link_to user.full_name, admin_user_path(user) %> - <%= user.email %>
 <%= link_to "Edit", edit_admin_user_path(user) %>
 <%= link_to "Delete", admin_user_path(user), method: :delete, :"data-confirm" => "You sure?" %>
 </p>
<% end %>
